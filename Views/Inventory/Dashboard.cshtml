
@{
    ViewBag.Title = "Dashboard";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<style>
    table.ws-table-all {
        margin: 20px 0;
        display: table;
        border: 1px solid #ccc;
    }

    .ws-table-all tr:nth-child(2n+1) {
        background-color: #fff;
    }

    .ws-table-all tr {
        border-bottom: 1px solid #ddd;
    }

    #customers th {
        padding-top: 11px;
        padding-bottom: 11px;
        background-color: #04AA6D;
        color: white;
    }

    #customers td, #customers th {
        text-align: left;
        padding: 8px;
    }

    .ws-table-all td, .ws-table-all th {
        padding: 8px 8px;
        display: table-cell;
        text-align: left;
        vertical-align: top;
    }

    .ws-table-all th {
        padding-top: 11px;
        padding-bottom: 11px;
        background-color: #04AA6D;
        color: white;
    }
</style>
<div class="row">
    <div class="col col-sm-6">
        @Html.Partial("~/Views/Inventory/_PartialCustomerList.cshtml")
    </div>
    <div class="col col-sm-6">
        @Html.Partial("~/Views/Inventory/_PartialEquipmentAssignedReturnList.cshtml")
    </div>
</div>

<div class="row">
    <div class="col col-sm-6">
        @Html.Partial("~/Views/Inventory/_PartialEquipmentList.cshtml")
    </div>
    <div class="col col-sm-6">
        @Html.Partial("~/Views/Inventory/_PartialEquipmentAssignedList.cshtml")
    </div>
</div>

@Html.Partial("~/Views/Inventory/_PartialModalEquipmentEntry.cshtml")
@Html.Partial("~/Views/Inventory/_PartialModalCustomerEntry.cshtml")





